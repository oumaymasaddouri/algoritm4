ALGORITHM Sorting 
VAR 
 i=INTEGER
    A= ARRAY_OF INTEGER;
    j=INTEGER
    x=INTEGER

   
BEGIN 
 i := 1
 while i < length(A)
    x := A[i]
    j := i - 1
    while j >= 0 and A[j] > x
        A[j+1] := A[j]
        j := j - 1
    end while
    A[j+1] := x[3]
    i := i + 1
 end while
END